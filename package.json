{
	"name": "qalculator",
	"version": "0.0.1",
	"scripts": {
		"lint-js": "eslint . --cache --ext .js,.jsx,.ts,.tsx,.json,.svelte --max-warnings 0",
		"check-ts": "tsc --noEmit",
		"lint-css": "stylelint **/*.css",
		"lint": "yarn lint-js; yarn check-ts; yarn lint-css; yarn prettier **/*.svelte --check",
		"fix": "yarn lint-js --fix; yarn check-ts; yarn lint-css --fix; yarn prettier **/*.svelte --write"
	},
	"workspaces": [
		"website"
	],
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-tonalio": "^5.1.4",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"svelte": "^3.46.4",
		"svelte-language-server": "^0.14.21",
		"svelte-preprocess": "^4.10.2",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	},
	"type": "module",
	"packageManager": "yarn@3.1.1"
}
